<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="stylesheet" href="lib/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="lib/css/clean-zone-style.css"/>
    <link rel="stylesheet" href="lib/font-awesome-4/css/font-awesome.css"/>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,400italic,700,800' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Raleway:300,200,100' rel='stylesheet' type='text/css'>
    <title><%= title %></title>
    <style>
        .music-ul {
            padding-left: 0px !important;
        }
        .music-li {
            margin: 0px !important;
        }
        .music-album-image {
            width: 80px;
            height: 80px;
            border-radius: 40px;
            margin-left: 30px;
            margin-right: 30px;
            box-shadow: 1px 1px 3px rgba(0,0,0,0.13);
        }
        .title-text-up {
            font-size: 25px;
        }
        .title-text-div {
            border-bottom: 1px solid #e8e8e8;
            background-color: #eee;
        }
        .share-div {
            margin: 0 -15px -20px -15px !important;
        }
    </style>
</head>
<body style="opacity: 1; margin-left: 0px;">
    <div id="cl-wrapper">
        <div class="container-fluid" id="pcont">
            <div class="cl-mcont">
                <div class="row dash-cols">
                    <div class="col-sm-2 col-md3"></div>
                    <div class="col-sm-8 col-md6">
                        <div class="pull-right share-div">
                            <button type="button" class="btn btn-default btn-facebook bg">
                                <a href="javascript:shareFacebook()">
                                    <i class="fa fa-facebook"></i>
                                </a>
                            </button>
                            <button type="button" class="btn btn-default btn-twitter bg">
                                <a href="javascript:shareTwitter()">
                                    <i class="fa fa-twitter"></i>
                                </a>
                            </button>
                            <div style="display: inline-block; margin-left: 5px;">
                                <a href="javascript:shareStory()">
                                    <img src="https://dev.kakao.com/sdk/js/resources/story/icon_small.png" style="width: 36px; height: 30px; margin-bottom: 5px;">
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-2 col-md3"></div>
                </div>
                <div class="row dash-cols">
                    <div class="col-sm-2 col-md3"></div>
                    <!-- Content goes here -->
                    <div class="col-sm-8 col-md6">
                        <div class="block">
                            <div class="content no-padding">
                                <div class="row title-text-div">
                                    <div class="col-md-2">
                                        <h3 class="big-text title-text-up" style="text-align: right">Rank</h3>
                                    </div>
                                    <div class="col-md-6">
                                        <h3 class="big-text title-text-up" style="text-align: center">Music</h3>
                                    </div>
                                    <div class="col-md-2">
                                        <h3 class="big-text title-text-up">Singer</h3>
                                    </div>
                                    <div class="col-md-2">
                                        <h3 class="big-text title-text-up">Score</h3>
                                    </div>
                                </div>
                                <div id="music-list" class="items">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-2 col-md3"></div>
                </div>
            </div>
        </div>
    </div>

    <script src="lib/js/jquery.min.js"></script>
    <script src="lib/js/bootstrap.min.js"></script>
    <script src="webjars/react/0.13.3/react.js"></script>
    <script src="webjars/react/0.13.3/JSXTransformer.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/numeral.js/1.4.5/numeral.min.js"></script>
    <script src="https://developers.kakao.com/sdk/js/kakao.story.min.js"></script>
    <script src="lib/js/sns.js"></script>
    <script src="jsx/music/music.js" type="text/jsx"></script>
    <script src="jsx/music/music-list.js" type="text/jsx"></script>
    <script src="jsx/music/music-more.js" type="text/jsx"></script>
    <script src="jsx/music/music-app.js" type="text/jsx"></script>
    <script src="jsx/music/init.js" type="text/jsx"></script>
    <script type="text/javascript">

    </script>
</body>
</html>